	      return {
                   'languages': ['C',
		             'C++',
				     'Java',
                                     'Go',
                                  'Nodejs']}

api.add_resource(Programming, '/')

if __name__ == '__main__':
 	app.run(host='10.0.1.59', port=90, debug=True)
